    "version": "0.2.0",
    "configurations": [
        {
            "name": "uat-insight-api",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/bin/api.js",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "uat"
            },
            "console": "internalConsole",
            "sourceMaps": false,
            "outputCapture": "std"
        }
    ]
}
